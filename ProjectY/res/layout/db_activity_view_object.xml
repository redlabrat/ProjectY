<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/DBActivityViewObject_LinearLayoutMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_weight="1"
    android:orientation="vertical" >

    <EditText
        android:id="@+id/DBActivityViewObject_ObjectData1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="14dp" >
    </EditText>

    <EditText
        android:id="@+id/DBActivityViewObject_ObjectData2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="14dp" >
    </EditText>

    <EditText
        android:id="@+id/DBActivityViewObject_ObjectData3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="14dp" >
    </EditText>

    <EditText
        android:id="@+id/DBActivityViewObject_ObjectData4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="14dp" >

    </EditText>

    <CheckBox
        android:id="@+id/DBActivityViewObject_ObjectCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
    </CheckBox>

    <Button
        android:id="@+id/DBActivityViewObject_BtnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginTop="30dip"
        android:text="back" />

</LinearLayout>